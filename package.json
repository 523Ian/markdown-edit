{
  "name": "markdown-edit",
  "version": "0.4.0",
  "description": "Online markdown editor/viewer",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/karma start --browsers Firefox --single-run",
    "browser": "browser-sync start --files ./* --server .",
    "watch": "watchify lib/index.js -dv -o bundle.js",
    "start": "npm run watch & npm run browser",
    "test:local": "karma start"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/georgeOsdDev/markdown-edit.git"
  },
  "keywords": [
    "markdown",
    "react",
    "marked",
    "GFM"
  ],
  "author": "Takeharu.Oshida",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/georgeOsdDev/markdown-edit/issues"
  },
  "homepage": "https://github.com/georgeOsdDev/markdown-edit",
  "devDependencies": {
    "babel-eslint": "^5.0.0-beta4",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babelify": "^7.2.0",
    "browser-sync": "^2.10.0",
    "browserify": "^12.0.1",
    "chai": "^3.4.1",
    "eslint": "^1.10.2",
    "eslint-plugin-react": "^3.11.2",
    "karma": "^0.13.15",
    "karma-browserify": "^4.4.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.1",
    "karma-cli": "0.1.1",
    "karma-firefox-launcher": "^0.1.7",
    "karma-mocha": "^0.2.1",
    "karma-safari-launcher": "^0.1.1",
    "karma-spec-reporter": "0.0.23",
    "mocha": "^2.3.4",
    "watchify": "^3.6.1"
  },
  "dependencies": {
    "brace": "^0.5.1",
    "flux": "^2.1.1",
    "highlight.js": "^8.9.1",
    "marked": "^0.3.5",
    "object-assign": "^4.0.1",
    "react": "^0.14.3",
    "react-ace": "^2.6.0",
    "superagent": "^1.4.0"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "compact": false,
          "presets": [
            "es2015",
            "react"
          ]
        }
      ]
    ]
  }
}
